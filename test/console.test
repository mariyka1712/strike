function test.console.info {
	assert.expects "0";
	console.info "this is an info message, %s" 'but nothing really to report' 1> /dev/null;
	assert.ok "$?";
}

function test.console.error {
	assert.expects "0";
	console.error "this is an error message, %s" 'but nothing is wrong' 2> /dev/null;
	assert.ok "$?";
}

function test.console.warn {
	assert.expects "0";
	console.warn "this is a warning, %s" 'but nothing really went wrong' 2> /dev/null;
	assert.ok "$?";
}

