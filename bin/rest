#!/usr/bin/env bash

. ~/.strike/boilerplate.sh;
boilerplate "$@";

# configure help
require 'help';
help.man.page "default" "rest.1";
help.parse "$@";

# set up the main delegate module as 'rest'
module="$process_name";

require 'rest';

#switch on strict mode for http requests
#forces the program to quit if curl exits with a non-zero
#exit code
http_strict="on";

rest $@;
